{
    "name": "progressbar-babel",
    "version": "1.0.0",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "dev-build": "npm run js & npm run sass",
        "dev-watch": "npm run js-watch & npm run sass-watch",
        "js": "babel src -d lib",
        "js-watch": "babel --watch src -d lib",
        "sass": "sass scss:css",
        "sass-watch": "sass --watch scss:css"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.0",
        "@babel/core": "^7.7.5",
        "@babel/preset-env": "^7.6.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.3.2",
        "file-loader": "^5.0.2",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "sass": "^1.23.7"
    }
}
