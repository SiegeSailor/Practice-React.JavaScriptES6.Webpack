{
    "name": "progressbar-babel",
    "version": "1.0.0",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "watch": "webpack --config webpack.config.js --watch --progress",
        "dev-build": "npm run js & npm run sass",
        "dev-watch": "npm run js-watch & npm run sass-watch",
        "js": "babel src -d lib",
        "js-watch": "babel --watch src -d lib",
        "sass": "sass scss:css",
        "sass-watch": "sass --watch scss:css"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.0",
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.6.0",
        "@babel/preset-react": "^7.7.4",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.3.2",
        "file-loader": "^5.0.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "@babel/runtime": "^7.7.6",
        "core-js": "^3.5.0",
        "sass": "^1.23.7",
        "whatwg-fetch": "^3.0.0"
    }
}
